<ParameterList name="Main" type="ParameterList">
  <ParameterList name="mesh" type="ParameterList">
    <Parameter isUsed="true" name="framework" type="string" value="MSTK" />
    <ParameterList name="generate mesh" type="ParameterList">
      <Parameter name="number of cells" type="Array(int)" value="{1,1,100}" />
      <Parameter name="domain low coordinate" type="Array(double)" value="{0.0,0.0,0.0}" />
      <Parameter name="domain high coordinate" type="Array(double)" value="{1.0,1.0,10.0}" />
      <Parameter name="Number of mesh blocks" type="int" value="1" />
      <ParameterList name="Mesh block 1" type="ParameterList">
        <Parameter name="Z0" type="double" value="0.0" />
        <Parameter name="Z1" type="double" value="10.0" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface mesh" type="ParameterList">
      <Parameter name="surface sideset name" type="string" value="surface" />
    </ParameterList>
  </ParameterList>

  <ParameterList name="regions" type="ParameterList">
    <ParameterList name="computational domain" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{1.0,1.0,10.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface domain" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{1.0,1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="east face" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{1.0,1.0,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{1.0,0.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="west face" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{0.0,0.0,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{-1.0,0.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="east edge" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{1.0,1.0}" />
        <Parameter name="normal" type="Array(double)" value="{1.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="west edge" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{-1.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="north face" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{1.0,1.0,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{0.0,1.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="south face" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{0.0,0.0,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{0.0,-1.0,0.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="bottom face" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{0.5,0.5,0.0}" />
        <Parameter name="normal" type="Array(double)" value="{0.0,0.0,-1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface" type="ParameterList">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{0.5,0.5,10.0}" />
        <Parameter name="normal" type="Array(double)" value="{0.0,0.0,1.0}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="cycle driver" type="ParameterList">
    <Parameter name="start time" type="double" value="0.0" />
    <Parameter name="start time units" type="string" value="s" />
    <Parameter name="end cycle" type="int" value="2" />
    <Parameter name="end time" type="double" value="2.0" />
    <Parameter name="end time units" type="string" value="d" />
    <ParameterList name="PK tree" type="ParameterList">
      <ParameterList name="Top level MPC" type="ParameterList">
        <Parameter name="PK type" type="string" value="weak MPC" />
        <ParameterList name="carbon budget" type="ParameterList">
          <Parameter name="PK type" type="string" value="BGC simple" />
        </ParameterList>
        <ParameterList name="water budget" type="ParameterList">
          <Parameter name="PK type" type="string" value="strong MPC" />
          <ParameterList name="canopy" type="ParameterList">
            <Parameter name="PK type" type="string" value="general surface balance" />
          </ParameterList>
          <ParameterList name="litter" type="ParameterList">
            <Parameter name="PK type" type="string" value="general surface balance" />
          </ParameterList>
          <ParameterList name="coupled water" type="ParameterList">
            <Parameter name="PK type" type="string" value="coupled water" />
            <ParameterList name="subsurface flow" type="ParameterList">
              <Parameter name="PK type" type="string" value="richards flow" />
            </ParameterList>
            <ParameterList name="surface flow" type="ParameterList">
              <Parameter name="PK type" type="string" value="overland flow, pressure basis" />
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>
  <ParameterList name="visualization" type="ParameterList">
    <Parameter name="file name base" type="string" value="visdump" />
    <Parameter name="times start period stop" type="Array(double)" value="{0.0,864.0,-1.0}" />
  </ParameterList>

  <ParameterList name="visualization surface" type="ParameterList">
    <Parameter name="file name base" type="string" value="visdump_surface" />
    <Parameter name="times start period stop" type="Array(double)" value="{0.0,864.0,-1.0}" />
  </ParameterList>
  <ParameterList name="PKs" type="ParameterList">

    <ParameterList name="Top level MPC" type="ParameterList">
      <Parameter name="PK type" type="string" value="weak MPC" />
      <Parameter name="PKs order" type="Array(string)" value="{carbon budget, water budget}" />

    </ParameterList>

    <ParameterList name="carbon budget" type="ParameterList">
      <Parameter name="PK type" type="string" value="BGC simple" />
      <Parameter name="primary variable" type="string" value="carbon" />
      <Parameter name="initial time step" type="double" value="86400.0" />
      <Parameter name="soil partition name" type="string" value="all" />
      <Parameter name="cryoturbation mixing coefficient [cm^2/yr]" type="double" value="0.0" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="low" />
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <Parameter name="constant" type="double" value="0.0" />
      </ParameterList>

      <ParameterList name="leaf biomass initial condition" type="ParameterList">
        <Parameter name="constant" type="double" value="2.0" />
      </ParameterList>

      <ParameterList name="pft parameters" type="ParameterList">
        <ParameterList name="conifer" type="ParameterList">
          <Parameter name="Vcmax25 [micromol CO2 / m^2(leaf) s]" type="double" value="50.0" />
          <Parameter name="Emax25 [micromol C / m^2(leaf) s]" type="double" value="5.0" />
          <Parameter name="SLA [m^2(leaf) / kg C" type="double" value="9.2" />
          <Parameter name="max root depth [m]" type="double" value="2.0" />
          <Parameter name="evergreen" type="bool" value="true" />
          <Parameter name="ratio of leaf to root [-]" type="double" value="1.0" />
          <Parameter name="ratio of leaf to stem [-]" type="double" value="0.0343" />
          <Parameter name="stem longevity [yr]" type="double" value="200.0" />
          <Parameter name="leaf longevity [yr]" type="double" value="4.0" />
          <Parameter name="root longevity [yr]" type="double" value="4.0" />
        </ParameterList>
        <ParameterList name="grass" type="ParameterList">
          <Parameter name="Vcmax25 [micromol CO2 / m^2(leaf) s]" type="double" value="50.0" />
          <Parameter name="Emax25 [micromol C / m^2(leaf) s]" type="double" value="5.0" />
          <Parameter name="SLA [m^2(leaf) / kg C" type="double" value="10.0" />
          <Parameter name="max root depth [m]" type="double" value="0.5" />
          <Parameter name="evergreen" type="bool" value="true" />
          <Parameter name="ratio of leaf to root [-]" type="double" value="0.5" />
          <Parameter name="ratio of leaf to stem [-]" type="double" value="0.5" />
          <Parameter name="stem longevity [yr]" type="double" value="5.0" />
          <Parameter name="leaf longevity [yr]" type="double" value="1.0" />
          <Parameter name="root longevity [yr]" type="double" value="1.0" />
        </ParameterList>
      </ParameterList>

      <ParameterList name="soil carbon parameters" type="ParameterList">
        <Parameter name="mesh partition" type="string" value="all" />
        <ParameterList name="computational domain" type="ParameterList">
          <Parameter name="percent sand" type="double" value="40.0" />
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="water budget" type="ParameterList">
      <Parameter name="PK type" type="string" value="strong MPC" />
      <Parameter name="PKs order" type="Array(string)" value="{canopy,litter,coupled water}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="time integrator" type="ParameterList">
        <Parameter name="extrapolate initial guess" type="bool" value="true" />
        <Parameter name="solver type" type="string" value="nka_bt_ats" />
        <Parameter name="timestep controller type" type="string" value="smarter" />
        <ParameterList name="nka_bt_ats parameters" type="ParameterList">
          <Parameter name="nka lag iterations" type="int" value="2" />
          <Parameter name="max backtrack steps" type="int" value="5" />
          <Parameter name="backtrack lag" type="int" value="0" />
          <Parameter name="backtrack factor" type="double" value="0.5" />
          <Parameter name="backtrack tolerance" type="double" value="1e-05" />
          <Parameter name="nonlinear tolerance" type="double" value="0.0001" />
          <Parameter name="diverged tolerance" type="double" value="100000.0" />
          <Parameter name="limit iterations" type="int" value="21" />
          <ParameterList name="verbose object" type="ParameterList">
            <Parameter name="verbosity level" type="string" value="extreme" />
          </ParameterList>
        </ParameterList>
        <ParameterList name="timestep controller smarter parameters" type="ParameterList">
          <Parameter name="max iterations" type="int" value="18" />
          <Parameter name="min iterations" type="int" value="10" />
          <Parameter name="time step reduction factor" type="double" value="0.5" />
          <Parameter name="time step increase factor" type="double" value="1.25" />
          <Parameter name="max time step" type="double" value="128.0" />
          <Parameter name="min time step" type="double" value="1e-10" />
          <Parameter name="growth wait after fail" type="int" value="2" />
          <Parameter name="count before increasing increase factor" type="int" value="2" />
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="canopy" type="ParameterList">
      <Parameter name="PK type" type="string" value="general surface balance" />
      <Parameter name="primary variable" type="string" value="canopy-water_content" />
      <Parameter name="conserved quantity key" type="string" value="canopy-water_content" />
      <Parameter name="layer name" type="string" value="canopy" />
      <Parameter name="domain name" type="string" value="surface" />
      <Parameter name="max time step" type="double" value="8640.0" />
      <Parameter name="debug cells" type="Array(int)" value="{0}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="initial head" type="ParameterList">
            <Parameter name="region" type="string" value="surface domain" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="0.0" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="litter" type="ParameterList">
      <Parameter name="PK type" type="string" value="general surface balance" />
      <Parameter name="primary variable" type="string" value="litter-water_content" />
      <Parameter name="conserved quantity key" type="string" value="litter-water_content" />
      <Parameter name="layer name" type="string" value="litter" />
      <Parameter name="domain name" type="string" value="surface" />
      <Parameter name="max time step" type="double" value="8640.0" />
      <Parameter name="debug cells" type="Array(int)" value="{0}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="initial head" type="ParameterList">
            <Parameter name="region" type="string" value="surface domain" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="0.0" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="coupled water" type="ParameterList">
      <Parameter name="PK type" type="string" value="coupled water" />
      <Parameter name="PKs order" type="Array(string)" value="{subsurface flow,surface flow}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="preconditioner" type="ParameterList">
        <Parameter name="preconditioner type" type="string" value="boomer amg" />
        <ParameterList name="boomer amg parameters" type="ParameterList">
          <Parameter name="cycle applications" type="int" value="1" />
          <Parameter name="smoother sweeps" type="int" value="4" />
          <Parameter name="strong threshold" type="double" value="0.95" />
          <Parameter name="tolerance" type="double" value="0.0" />
        </ParameterList>
      </ParameterList>

      <ParameterList name="linear solver" type="ParameterList">
        <Parameter name="iterative method" type="string" value="gmres" />
        <ParameterList name="verbose object" type="ParameterList">
          <Parameter name="verbosity level" type="string" value="high" />
        </ParameterList>
        <ParameterList name="gmres parameters" type="ParameterList">
          <Parameter name="preconditioning strategy" type="string" value="left" />
          <Parameter name="error tolerance" type="double" value="1e-06" />
          <Parameter name="maximum number of iterations" type="int" value="20" />
          <ParameterList name="verbose object" type="ParameterList">
            <Parameter name="verbosity level" type="string" value="high" />
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="water delegate" type="ParameterList">
        <Parameter name="modify predictor damp and cap the water spurt" type="bool" value="true" />
        <Parameter name="damp and cap the water spurt" type="bool" value="true" />
        <Parameter name="cap over atmospheric" type="double" value="0.1" />
        <ParameterList name="verbose object" type="ParameterList">
          <Parameter name="verbosity level" type="string" value="high" />
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="surface flow" type="ParameterList">
      <Parameter name="PK type" type="string" value="overland flow, pressure basis" />
      <Parameter name="primary variable" type="string" value="surface-pressure" />
      <Parameter name="domain name" type="string" value="surface" />
      <Parameter name="source term" type="bool" value="true" />
      <Parameter name="source key" type="string" value="surface-mass_source" />
      <Parameter name="mass source in meters" type="bool" value="false" />
      <Parameter name="initial time step" type="double" value="1.0" />
      <Parameter name="debug cells" type="Array(int)" value="{0}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="diffusion" type="ParameterList">
        <Parameter name="discretization primary" type="string" value="fv: default" />
      </ParameterList>

      <ParameterList name="boundary conditions" type="ParameterList">
        <ParameterList name="seepage face head" type="ParameterList">
          <ParameterList name="surface" type="ParameterList">
            <Parameter name="regions" type="Array(string)" value="{east edge, west edge}" />
            <ParameterList name="boundary head" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="0.5" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <Parameter name="initialize surface head from subsurface" type="bool" value="true" />
      </ParameterList>

      <ParameterList name="overland conductivity evaluator" type="ParameterList">
        <Parameter name="include density factor" type="bool" value="true" />
        <ParameterList name="overland conductivity model" type="ParameterList">
          <Parameter name="overland conductivity type" type="string" value="manning harmonic mean" />
          <Parameter name="Manning exponent" type="double" value="0.667" />
          <Parameter name="slope regularization epsilon" type="double" value="0.01" />
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="subsurface flow" type="ParameterList">
      <Parameter name="PK type" type="string" value="richards flow" />
      <Parameter name="primary variable" type="string" value="pressure" />
      <Parameter name="initial time step" type="double" value="1.0" />
      <Parameter name="relative permeability method" type="string" value="upwind with Darcy flux" />
      <Parameter name="permeability rescaling" type="double" value="10000000.0" />
      <Parameter name="source term" type="bool" value="true" />
      <Parameter name="mass source key" type="string" value="transpiration" />
      <Parameter name="source term is differentiable" type="bool" value="false" />
      <Parameter name="debug cells" type="Array(int)" value="{70,99}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="high" />
      </ParameterList>

      <ParameterList name="diffusion" type="ParameterList">
        <Parameter name="discretization primary" type="string" value="mfd: default" />
      </ParameterList>

      <ParameterList name="boundary conditions" type="ParameterList">
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="initial pressure cells" type="ParameterList">
            <Parameter name="region" type="string" value="computational domain" />
            <Parameter name="components" type="Array(string)" value="{cell,face}" />
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-linear" type="ParameterList">
                <Parameter name="x0" type="Array(double)" value="{0.0,0.0,0.0,7.0}" />
                <Parameter name="y0" type="double" value="101325.0" />
                <Parameter name="gradient" type="Array(double)" value="{0.0,0.0,0.0,-9806.65}" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="water retention evaluator" type="ParameterList">
        <Parameter name="minimum rel perm cutoff" type="double" value="0.0" />
        <Parameter name="use surface rel perm" type="bool" value="true" />
        <ParameterList name="WRM parameters" type="ParameterList">
          <ParameterList name="computational domain" type="ParameterList">
            <Parameter name="region" type="string" value="computational domain" />
            <Parameter name="WRM Type" type="string" value="van Genuchten" />
            <Parameter name="van Genuchten alpha" type="double" value="0.00056" />
            <Parameter name="van Genuchten m" type="double" value="0.24" />
            <Parameter name="residual saturation" type="double" value="0.18" />
            <Parameter name="smoothing interval width [saturation]" type="double" value="0.1" />
          </ParameterList>
        </ParameterList>
      </ParameterList>

    </ParameterList>

  </ParameterList>

  <ParameterList name="state" type="ParameterList">
    <ParameterList name="mesh partitions" type="ParameterList">
      <ParameterList name="all" type="ParameterList">
        <Parameter name="region list" type="Array(string)" value="{computational domain}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="field evaluators" type="ParameterList">

      <ParameterList name="canopy-source_sink" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="additive evaluator" />
        <Parameter name="evaluator dependencies" type="Array(string)" value="{canopy-interception, canopy-drainage}" />
        <Parameter name="canopy-interception coefficient" type="double" value="1.0" />
        <Parameter name="canopy-drainage coefficient" type="double" value="-1.0" />
      </ParameterList>

      <ParameterList name="canopy-drainage" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="interception drainage" />
        <Parameter name="layer name" type="string" value="canopy" />
        <Parameter name="drainage timescale [s]" type="double" value="1200.0" />
        <Parameter name="saturated moisture content [m^3 H20 / m^2 biomass area]" type="double" value="0.0002" />
        <Parameter name="wet layer from surface water" type="bool" value="false" />
        <Parameter name="density of liquid water [mol/m^3]" type="double" value="55389.0" />
        <Parameter name="area index key" type="string" value="total_leaf_area_index" />
      </ParameterList>

      <ParameterList name="canopy-interception" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="interception/throughfall" />
        <Parameter name="layer name" type="string" value="canopy" />
        <Parameter name="source key" type="string" value="precipitation_rain" />
        <Parameter name="source in meters" type="bool" value="true" />
        <Parameter name="throughfall coefficient [m^2 / m^2 biomass area]" type="double" value="0.2" />
        <Parameter name="density of liquid water [mol/m^3]" type="double" value="55389.0" />
        <Parameter name="area index key" type="string" value="total_leaf_area_index" />
      </ParameterList>

      <ParameterList name="litter-source_sink" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="additive evaluator" />
        <Parameter name="evaluator dependencies" type="Array(string)" value="{litter-interception, litter-drainage}" />
        <Parameter name="litter-interception coefficient" type="double" value="1.0" />
        <Parameter name="litter-drainage coefficient" type="double" value="-1.0" />
      </ParameterList>

      <ParameterList name="litter-drainage" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="interception drainage" />
        <Parameter name="layer name" type="string" value="litter" />
        <Parameter name="drainage timescale [s]" type="double" value="600.0" />
        <Parameter name="saturated moisture content [m^3 H20 / m^2 biomass area]" type="double" value="0.0002" />
        <Parameter name="wet layer from surface water" type="bool" value="true" />
        <Parameter name="density of liquid water [mol/m^3]" type="double" value="55389.0" />
      </ParameterList>

      <ParameterList name="litter-interception" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="interception/throughfall" />
        <Parameter name="layer name" type="string" value="litter" />
        <Parameter name="source key" type="string" value="litter-source" />
        <Parameter name="source in meters" type="bool" value="false" />
        <Parameter name="throughfall coefficient [m^2 / m^2 biomass area]" type="double" value="0.1" />
        <Parameter name="density of liquid water [mol/m^3]" type="double" value="55389.0" />
      </ParameterList>

      <ParameterList name="litter-source" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="additive evaluator" />
        <Parameter name="evaluator dependencies" type="Array(string)" value="{precipitation_rain, canopy-interception, canopy-drainage}" />
        <Parameter name="precipitation_rain coefficient" type="double" value="55389.0" />
        <Parameter name="canopy-interception coefficient" type="double" value="-1.0" />
        <Parameter name="canopy-drainage coefficient" type="double" value="1.0" />
      </ParameterList>

      <ParameterList name="litter-area_index" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="additive evaluator" />
        <Parameter name="evaluator dependencies" type="Array(string)" value="{litter-thickness}" />
        <Parameter name="litter-thickness coefficient" type="double" value="15.24" />
      </ParameterList>

      <ParameterList name="litter-thickness" type="ParameterList">
        <Parameter name="field evaluator type" type="string" value="independent variable" />
        <Parameter name="constant in time" type="bool" value="true" />
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="cells" type="ParameterList">
            <Parameter name="region" type="string" value="surface domain" />
            <Parameter name="component" type="string" value="cell" />
