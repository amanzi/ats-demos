<ParameterList name="Main" type="ParameterList">

  <ParameterList name="mesh">
    <ParameterList name="domain">
      <Parameter name="deformable mesh" type="bool" value="false" />
      <Parameter name="mesh type" type="string" value="read mesh file" />
      <ParameterList name="read mesh file parameters">
        <Parameter name="file" type="string" value="../meshes/box_mesh_5000.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface">
      <Parameter name="deformable mesh" type="bool" value="false" />
      <Parameter name="mesh type" type="string" value="surface" />
      <ParameterList name="surface parameters">
        <Parameter name="surface sideset names" type="Array(string)" value="{top face}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Domain">
    <Parameter name="Spatial Dimension" type="int" value="3" />
  </ParameterList>
  <ParameterList name="regions">
    <ParameterList name="computational domain">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{5000.0,1.0,100.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="latteral face">
      <ParameterList name="region: labeled set">
        <Parameter name="entity" type="string" value="face" />
        <Parameter isUsed="true" name="file" type="string" value="../meshes/box_mesh_5000.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
        <Parameter name="label" type="string" value="3" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="bottom face">
      <ParameterList name="region: labeled set">
        <Parameter name="entity" type="string" value="face" />
        <Parameter isUsed="true" name="file" type="string" value="../meshes/box_mesh_5000.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
        <Parameter name="label" type="string" value="1" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="top face">
      <ParameterList name="region: labeled set">
        <Parameter name="entity" type="string" value="face" />
        <Parameter isUsed="true" name="file" type="string" value="../meshes/box_mesh_5000.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
        <Parameter name="label" type="string" value="2" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface domain">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{5000.0,1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="rain domain">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{5000.0,1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface outlet">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{0.0,1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface west">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{0.0,1.0}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="surface east">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{5000.0,0.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{5000.0,1.0}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>


  <ParameterList name="verbose object">
    <Parameter name="verbosity level" type="string" value="high"/>
  </ParameterList>

  <ParameterList name="cycle driver" type="ParameterList">
    <Parameter name="start time" type="double" value="0." />
    <Parameter name="start time units" type="string" value="s" />
    <!-- <Parameter name="end cycle" type="int" value="10" /> -->
    <Parameter name="end time" type="double" value="3600" />
    <Parameter name="end time units" type="string" value="s" />
    <!-- <Parameter name="restart from checkpoint file" type="string" value="restart.h5" /> -->
    <ParameterList name="PK tree" type="ParameterList">
      <ParameterList name="Overland Flow" type="ParameterList">
        <Parameter name="PK type" type="string" value="overland flow, pressure basis" />
      </ParameterList>      
    </ParameterList>

    <ParameterList name="verbose object">
      <Parameter name="verbosity level" type="string" value="high"/>
    </ParameterList>
   
  </ParameterList>
  
  <ParameterList name="PKs" type="ParameterList">
    
    <ParameterList name="GeoMorphology" type="ParameterList">
      <Parameter name="PK type" type="string" value="morphology" />
      <Parameter name="PKs order" type="Array(string)" value="{Overland Flow, Sediment Transport}" />
      <Parameter name="domain name"  type="string" value="surface" />
      <Parameter name="number of vegitation species" type="int" value="1" />
      <!-- <Parameter name="dt sample" type="double" value="86400" /> -->
      <!-- <Parameter name="dt MPC" type="double" value="31557600" /> -->
      <Parameter name="dt sample" type="double" value="-1" />
      <Parameter name="dt MPC" type="double" value="-1" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="extreme" />
        <Parameter name="name" type="string" value="GeoMorphology" />
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="Overland Flow" type="ParameterList">
      <Parameter name="PK type" type="string" value="overland flow, pressure basis" />
      <Parameter name="primary variable key" type="string" value="surface-pressure" />
      <Parameter name="domain name" type="string" value="surface" />
      <Parameter name="source term" type="bool" value="true" />
      <Parameter name="debug cells" type="Array(int)" value="{0,9}" />
      <Parameter name="initial time step" type="double" value="1" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="low" />
      </ParameterList>

      <ParameterList name="diffusion" type="ParameterList">
        <Parameter name="discretization primary" type="string" value="fv: default" />
      </ParameterList>

      <ParameterList name="diffusion preconditioner" type="ParameterList">
        <Parameter name="discretization primary" type="string" value="fv: default" />
        <Parameter name="Newton correction" type="string" value="true Jacobian"/>
      </ParameterList>

      <ParameterList name="preconditioner" type="ParameterList">
        <Parameter name="preconditioner type" type="string" value="block ilu" />
      </ParameterList>

      <ParameterList name="time integrator" type="ParameterList">
        <Parameter name="extrapolate initial guess" type="bool" value="false" />
        <Parameter name="solver type" type="string" value="nka" />
        <!-- <Parameter name="timestep controller type" type="string" value="from file" /> -->
        <Parameter name="timestep controller type" type="string" value="smarter" />        
        <ParameterList name="nka parameters" type="ParameterList">
          <Parameter name="max error growth factor" type="double" value="100000000.0" />
          <Parameter name="modify correction" type="bool" value="true" />
          <Parameter name="monitor" type="string" value="monitor residual" />
          <Parameter name="nonlinear tolerance" type="double" value="1e-5" />
          <Parameter name="diverged tolerance" type="double" value="10000000000.0" />
          <Parameter name="limit iterations" type="int" value="100" />
          <ParameterList name="verbose object" type="ParameterList">
            <Parameter name="verbosity level" type="string" value="high" />
          </ParameterList>
        </ParameterList>
        <ParameterList name="verbose object" type="ParameterList">
          <Parameter name="verbosity level" type="string" value="high" />
        </ParameterList>
        <ParameterList name="timestep controller smarter parameters" type="ParameterList">
          <Parameter name="max iterations" type="int" value="18" />
          <Parameter name="min iterations" type="int" value="10" />
          <Parameter name="time step reduction factor" type="double" value="0.5" />
          <Parameter name="time step increase factor" type="double" value="1.25" />
          <Parameter name="max time step" type="double" value="200" />
          <Parameter name="min time step" type="double" value="1e-10" />
          <Parameter name="growth wait after fail" type="int" value="2" />
          <Parameter name="count before increasing increase factor" type="int" value="2" />
        </ParameterList>
        <!-- <ParameterList name="timestep controller from file parameters" type="ParameterList"> -->
        <!--   <Parameter name="file name" type="string" value="../data/surface_water_pressure-fv_dts.h5" /> -->
        <!-- </ParameterList> -->
      </ParameterList>

      <ParameterList name="linear solver" type="ParameterList">
        <Parameter name="iterative method" type="string" value="nka" />
        <ParameterList name="verbose object" type="ParameterList">
          <Parameter name="verbosity level" type="string" value="none" />
        </ParameterList>
        <ParameterList name="nka parameters" type="ParameterList">
          <Parameter name="error tolerance" type="double" value="1e-06" />
          <Parameter name="maximum number of iterations" type="int" value="5" />
          <ParameterList name="verbose object" type="ParameterList">
            <Parameter name="verbosity level" type="string" value="none" />
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="overland conductivity evaluator" type="ParameterList">
        <ParameterList name="overland conductivity model" type="ParameterList">
          <Parameter name="Manning exponent" type="double" value="0.666666666667" />
          <!-- <Parameter name="Manning exponent" type="double" value="0.9" /> -->
          <Parameter name="slope regularization epsilon" type="double" value="0.0001" />
        </ParameterList>
      </ParameterList>

      <ParameterList name="initial condition" type="ParameterList">
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="initial head" type="ParameterList">
            <Parameter name="region" type="string" value="surface domain" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="101325.0" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="boundary conditions" type="ParameterList">
        <ParameterList name="mass flux"> 
          <ParameterList name="BC west">
            <Parameter name="regions" type="Array(string)" value="{surface west}"/>
            <ParameterList name="outward mass flux">
              <ParameterList name="function-tabular">
                <Parameter name="file" type="string" value="../floodplain1.h5" />
                <Parameter name="x header" type="string" value="Time" />
                <Parameter name="y header" type="string" value="Flux" />
                <Parameter name="form" type="Array(string)" value="{constant}"/>
              </ParameterList>
            </ParameterList>            
          </ParameterList>
        </ParameterList>
      </ParameterList>

    </ParameterList>

    <ParameterList name="Sediment Transport" type="ParameterList">
      <Parameter name="PK type" type="string" value="sediment transport" />
      <Parameter name="domain name" type="string" value="surface" />
      <Parameter name="saturation liquid key" type="string" value="surface-ponded_depth" />
      <Parameter name="previous saturation liquid key" type="string" value="surface-prev_ponded_depth" />
      <Parameter name="mass flux" type="string" value="mass_flux" />      
      <Parameter name="component names" type="Array(string)" value="{sediment}" />
      <Parameter name="number of liquid components" type="int" value="1" />
      <Parameter name="component molar masses" type="Array(double)" value="{1.0}" />
      <Parameter name="spatial discretization order" type="int" value="1" />
      <Parameter name="temporal discretization order" type="int" value="1" />
      <Parameter name="cfl" type="double" value="1.0" />
      <!-- <Parameter name="solver" type="string" value="PCG with Hypre AMG" /> -->
      <Parameter name="enable internal tests" type="string" value="no" />
      <Parameter name="solver" type="string" value="diffusion solver" />      
      <Parameter name="transport subcycling" type="bool" value="false" />
      <Parameter name="number of aqueous components" type="int" value="1" />
      <Parameter name="number of gaseous components" type="int" value="0" />
      <Parameter name="water tolerance" type="double" value="0.00001" />
      <Parameter name="runtime diagnostics: regions" type="Array(string)" value="{}" />
      <ParameterList name="verbose object" type="ParameterList">
        <Parameter name="verbosity level" type="string" value="low" />
        <Parameter name="name" type="string" value="Sediment Transport" />
      </ParameterList>

      <ParameterList name="reconstruction" type="ParameterList">
        <Parameter name="polynomial order" type="int" value="0" />
        <Parameter name="limiter" type="string" value="tensorial" />
        <Parameter name="limiter extension for transport" type="bool" value="true" />
      </ParameterList>

      <ParameterList name="operators">
        <ParameterList name="diffusion operator">
          <ParameterList name="matrix">
            <Parameter name="discretization primary" type="string" value="fv: default"/>
            <Parameter name="discretization secondary" type="string" value="fv: default"/>
            <Parameter name="schema" type="Array(string)" value="{cell}"/>
            <Parameter name="gravity" type="bool" value="false"/>
          </ParameterList>
        </ParameterList>
      </ParameterList>      

      <ParameterList name="source terms" type="ParameterList">
      </ParameterList>

      <ParameterList name="boundary conditions">
        <ParameterList name="concentration">
          <ParameterList name="sediment">
            <ParameterList name="surface west">   <!-- user defined name -->
              <Parameter name="regions" type="Array(string)" value="{surface west}"/>
              <Parameter name="spatial distribution method" type="string" value="none"/>
              <ParameterList name="boundary concentration">
                <ParameterList name="function-constant">  <!-- any time function -->
                  <Parameter name="value" type="double" value="3.77358492e-06"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
      
    </ParameterList>

 

  </ParameterList>

